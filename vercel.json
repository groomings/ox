{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/versatile/versatile.html" },
    { "source": "/versatile", "destination": "/versatile/versatile.html" },
    { "source": "/nate", "destination": "/nate/nate.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|mp3|cur)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
